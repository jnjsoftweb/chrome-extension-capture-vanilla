{
  "manifest_version": 3,
  "name": "배경색 변경 확장 프로그램",
  "version": "1.0",
  "description": "웹 페이지의 배경색을 변경하는 확장 프로그램",
  "permissions": ["activeTab", "scripting"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  }
}
